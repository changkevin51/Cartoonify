{
  "manifest_version": 3,
  "name": "Cartoonify",
  "version": "1.0",
  "description": "A Chrome extension that cartoonifies web pages",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "media/icon.png",
      "48": "media/icon.png",
      "128": "media/icon.png"
    }
  },
  "web_accessible_resources": [{
  "resources": ["media/doodles.jpg", "media/turtle.gif", "media/pig.png", "media/spongebob.png", "media/Boing.mp3", "media/Resonance.mp3"], 
    "matches": ["<all_urls>"]
  }]
}